<table id="example2" class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>No</th>
      <th>Nama</th>
      <th>Aksi(s)</th>

    </tr>
  </thead>
  <tbody>
    <% for( let index = 0; index < category.length; index++ ) { %>
    <tr>
      <td> <%= index+1 %> </td>
      <td> <%= category[index].name %> </td>
      <td> <form action="/category/delete/<%= category[index]._id %>?_method=DELETE" method="post">
        <a href="/category/edit/<%= category[index]._id %> " class="btn btn-warning"> <i class="fas fa-pencil-alt"></i>  Ubah</a>
        <button class="btn btn-danger"><i class="fas fa-trash"></i>  Hapus</button>
      </form> </td>


    </tr>
    <% } %>

  </tbody>

</table>